{
	"main": "main.js",
	"version": "1.0.0",
	"author": "shohei909",
	"description": "Viewer for Visual Log(.vilog)",
	"name": "VisualLogViewer",
	"dependencies": {
		"golden-layout": "^1.5.9"
	},
	"devDependencies": {
		"electron": "9.2.0",
		"electron-builder": "22.4.0"
	},
	"build": {
		"extraMetadata": {
			"main": "./main.js"
		},
		"appId": "net.corge.vilog.viewer",
		"fileAssociations": [
			{
				"ext": ["vilog"],
				"name": "VisualLog",
				"role": "Viewer"
			}
		],
		"files": [
			"index.html",
			"icon.png",
			"main.js",
			"renderer.js",
			"vilog.min.js",
			"*.css",
			"fonts/*"
		]
	},
	"license": "MIT"
}
